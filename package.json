{
  "name": "Socket-Server-Template",
  "version": "1.0.0",
  "description": "Web Socket Node Server Example",
  "main": "main.js",
  "author": "Torin Blankensmith",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node ./main.js",
    "dev": "NODE_ENV=development PORT=5000 node ./main.js"
  },
  "engines": {
    "node": "17"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^3.3.2",
    "uuid": "^8.3.2",
    "ws": "^8.2.2"
  },
  "headers": {
    "/webs/style.css": {
      "Content-Type": "text/css"
    },
    "/webs/script.js": {
      "Content-Type": "application/javascript"
    },
    "/webs/*.html": {
      "Content-Type": "text/html"
    }
  }
}
